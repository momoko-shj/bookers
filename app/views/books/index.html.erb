<html>
<head>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>  

<body>
  <h1>Books</h1>
  
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Body </th>
        <th colspan="3"></th>
      </tr>
      </thead>
      <!--ここからエラー要素が潜んでいる範囲　外すと投稿画面が表示されます-->
      <tbody>
      <% @books.each do |book| %>
      <td>
        <%= f.text_area :body %>
      </td>
      <td>
        <%= f.text_field :title %>
      </td>
    </tbody>
  </table>
      <!--ここまで-->
      
 <h2>New Book</h2>
  <%= form_with model: @book do |f| %>
  <p>Title</p>
  <%= f.text_field:title %>
  
  <p>Body</p>
  <%=f.text_area :body %>
  
  <%=f.submit 'Create Book' %>
  <% end %>
  
</body>
</html>